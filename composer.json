{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.5.3",
		"silverstripe/framework": "3.5.3",
		"silverstripe/reports": "3.5.3",
		"silverstripe/siteconfig": "^3.6",
		"silverstripe-themes/simple": "3.1.*",
		"silvershop/core": "^2.2",
		"burnbright/silverstripe-bootstrap": "^1.0",
		"silvershop/shipping": "^0.2.1",
		"silvershop/discounts": "^1.2",
		"omnipay/manual": "^2.2",
		"omnipay/dummy": "^2.1",
		"silverstripe/subsites": "^1.3",
		"silverstripe/silverstripe-omnipay": "^2.2"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.5.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
